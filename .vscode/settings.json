{
	"cSpell.words": ["bigserial", "Partitioner", "Partitioners"],
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"editor.fontSize": 15,
	// Restore Terminal;
	"restoreTerminals.terminals": [
		{
			"splitTerminals": [
				{
					"name": "upload",
					"commands": ["cd servers/upload", "yarn dev"]
				},
				{
					"name": "transcoder",
					"commands": ["cd servers/transcoder", "yarn dev"]
				},
				{
					"name": "watch",
					"commands": ["cd servers/watch", "yarn dev"]
				},
				{
					"name": "websocket",
					"commands": ["cd servers/websocket", "yarn dev"]
				}
			]
		},
		{
			"splitTerminals": [
				{
					"name": "websocket",
					"commands": ["cd client", "yarn dev"]
				},
				{
					"name": "websocket",
					"commands": ["docker compose up -d"]
				}
			]
		}
	]
