		"express": "^4.19.2",
		"kafkajs": "^2.2.4",
		"multer": "^1.4.5-lts.1",
		"pg": "^8.12.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/multer": "^1.4.11",
		"@types/node": "^22.0.0",
		"@types/pg": "^8.11.6",
		"drizzle-kit": "^0.23.0",
		"esbuild": "^0.23.0",
		"esbuild-register": "^3.6.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.5.4"
